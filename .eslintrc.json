{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
      "global-require": "off",
      "no-param-reassign": "off",
      "new-cap": "off",
      "import/no-dynamic-require": "off"
    }
}
